{
  "name": "fruit-core",
  "version": "0.1.0",
  "homepage": "https://github.com/fruitstudios/core",
  "description": "Core is a project boilerplate for Craft 3",
  "author": "Sean Hill",
  "main": "gulpfile.js",
  "license": "MIT",
  "paths": {
    "src": {
      "styles": "src/scss/app.scss",
      "images": "src/images/**/*",
      "fonts": "src/fonts/*",
      "svgs": "src/icons/svg/**/*.svg",
      "js": [
        "src/js/vendor/**/*.js",
        "src/js/app.js",
        "src/js/modules/**/*.js"
      ]
    },
    "build": {
      "styles": "web/build/css/",
      "images": "web/build/images/",
      "fonts": "web/build/fonts/",
      "svgs": "web/build/icons/svg/",
      "js": "web/build/js/"
    }
  },
  "urls": {
    "live": "",
    "local": ""
  },
  "devDependencies": {
    "browser-sync": "^2.17.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-html-beautify": "^1.0.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-svg-symbols": "1.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "pips.css": "^0.1.3"
  }
}
